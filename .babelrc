{
  "plugins" : [
    [
      "import",
      [
        { "libraryName": "antd-mobile" },
        { "libraryName": "mulan-lib", "libraryDirectory":"lib" },
        {
          "libraryName": "ramda",
          "libraryDirectory": "src",
          "camel2DashComponentName": false
        },
      ],
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "lib": "./lib",
          "store": "./store"
        }
      }
    ],
  ],
  "presets": ["next/babel"]
}
